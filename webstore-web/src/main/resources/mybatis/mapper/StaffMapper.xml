<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zx.mapper.ServiceMapper">

    <resultMap id="loginMap" type="cn.iamtudou.entity.StaffEntity">
        <result column="telephone" property="telephone" jdbcType="String" />
        <result column="password" property="password" jdbcType="String" />
    </resultMap>
    
    <select id="staffLogin" resultMap="loginMap" parameterType="String">
        select telephone, password from user_staff where telephone = #{telephone}
    </select>
</mapper>